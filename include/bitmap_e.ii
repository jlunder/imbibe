inline void bitmap_element::b(bitmap * n_b)
{
  m_b = n_b;
  repaint();
}


inline bitmap & bitmap_element::b()
{
  return *m_b;
}


inline bitmap const & bitmap_element::b() const
{
  return *m_b;
}


