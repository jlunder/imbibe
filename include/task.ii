#ifndef __TASK_II_INCLUDED
#define __TASK_II_INCLUDED


#include "task_manager.ii"


inline void task::start()
{
  m_owner.add_task(*this);
}


inline void task::stop()
{
  m_owner.remove_task(*this);
}


inline task_manager & task::owner()
{
  return m_owner;
}


#endif //__TASK_II_INCLUDED


